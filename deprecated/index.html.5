  // ---- 8/24 Hero Experiment ----
  // Load hero videos
  $(window).on('load', function() {
    $('[data-hero-video]').each(function() {
      let heroVideo = $(this);
      heroVideo.find('source').each(function() {
        let dataSrc = $(this).attr('data-src');
        if (dataSrc) {
          $(this).attr('src', dataSrc);
          $(this).removeAttr('data-src');
        }
      });
      heroVideo[0].load();
      heroVideo[0].play();
    });
  });

  // Features (and 8/24 hero experiment!) section video play/pause functionality
  $('video').click(function () {
    var $playButton = $(this).closest('[data-video-wrapper]').find(
      '[data-video-playback="play"]');
    var $pauseButton = $(this).closest('[data-video-wrapper]').find(
      '[data-video-playback="pause"]');

    if (this.paused) {
      this.play();
      $playButton.css('display', 'none');
      $pauseButton.css('display', 'flex');
    } else {
      this.pause();
      $playButton.css('display', 'flex');
      $pauseButton.css('display', 'none');
    }
  });

  $('[data-video-playback]').click(function () {
    var video = $(this).closest('[data-video-wrapper]').find('video')[0];
    var $playButton = $(this).closest('[data-video-wrapper]').find(
      '[data-video-playback="play"]');
    var $pauseButton = $(this).closest('[data-video-wrapper]').find(
      '[data-video-playback="pause"]');

    if (video.paused) {
      video.play();
      $playButton.css('display', 'none');
      $pauseButton.css('display', 'flex');
    } else {
      video.pause();
      $playButton.css('display', 'flex');
      $pauseButton.css('display', 'none');
    }
  });

  //Composable cms buttons click code
  const composableButtons = document.querySelectorAll('.composable-button')
  composableButtons.forEach((button, index) => {
    button.addEventListener('click', () => {
      setTimeout(() =>{
        document.querySelectorAll('.composable-image').forEach(image => {
          image.style.opacity = 0
        })
        document.querySelectorAll('.composable-image')[index+1].style.opacity = 1
      }, 700)
    })
  })
</script>
