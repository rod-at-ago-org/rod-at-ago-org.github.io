<!-- Exit Modal Code -->
<script>
    const wf_localStorageSeenKey = "wf_exit_modal_seen_feb_24";
    const wf_exit_modal_exp_feb_24 = "growth_exit_modal_home_feb_24";
    const wf_modalTriggerId = "close-exp-exit-popup";

    function hasSeenModalLast24Hours() {
        const lastSeen = localStorage.getItem(wf_localStorageSeenKey);
        if (lastSeen) {
            var lastSeenDate = new Date(lastSeen);
            var now = new Date();
            var diff = now - lastSeenDate;
            var hours = diff / 1000 / 60 / 60;
            return hours < 24;
        }

        return false;
    }

    function setModalSeen() {
        localStorage.setItem(wf_localStorageSeenKey, new Date());
    }

    const wf_mousePosHandler = (e) => {
        if (!e.toElement && !e.relatedTarget && e.clientY < 0) {
            let isMouseOut = true;

            const onMouseEnter = () => {
                isMouseOut = false;
            };

            document.addEventListener("mouseenter", onMouseEnter);

            setTimeout(() => {
                // if mouse has entered the page again, don't show modal
                if (!isMouseOut) {
                    document.removeEventListener("mouseenter", onMouseEnter);
                    return;
                }

                document.removeEventListener("mouseout", wf_mousePosHandler);
                document.removeEventListener("mouseenter", onMouseEnter);

                var wf_triggerModal = document.getElementById(wf_modalTriggerId);

                if (!wf_triggerModal) {
                    return;
                }

                setModalSeen();
                wf_triggerModal.click();
                wf_analytics && wf_analytics.track("Exit Modal Viewed");
            }, 1000);
        }
    };

    window.addEventListener("load", function () {
        // on page load if url has ?show_exit_modal=true, show the modal
        if (window.location.search.includes("show_exit_modal=true")) {
            const button = document.getElementById(wf_modalTriggerId);
            if (button) {
                button.click();
            }
        }
    });

    if (!hasSeenModalLast24Hours()) {
        document.addEventListener("mouseout", wf_mousePosHandler);
    }
</script>
